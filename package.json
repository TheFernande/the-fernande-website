{
  "name": "the-fernande",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:analyze": "ANALYZE=true next build",
    "build": "next build",
    "clean": "rm -rf node_modules .yarn/cache .yarn/install-state.gz .next coverage",
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "lint:check": "echo '--- Running ESLint ---\n' && eslint --color --no-error-on-unmatched-pattern --config ./.eslintrc.json '{public,src}/**/*.{js,jsx,ts,tsx,json}';",
    "lint:fix": "echo '--- Running ESLint ---\n' && eslint --fix --color --config ./.eslintrc.json '{public,src}/**/*.{js,jsx,ts,tsx,json}';",
    "prepare": "husky install",
    "prettier:check": "prettier --check --config ./.prettierrc '{public,src}/**/*.{htm,html,js,jsx,ts,tsx,css,md}'",
    "prettier:fix": "prettier --write --list-different --config ./.prettierrc '{public,src}/**/*.{htm,html,js,jsx,ts,tsx,css,md}'",
    "preview": "next build && next start",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "next": "14.2.2",
    "react": "^18",
    "react-dom": "^18",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@emotion/is-prop-valid": "^1.2.2",
    "@formkit/auto-animate": "^0.8.2",
    "@next/bundle-analyzer": "^14.2.3",
    "@storybook/addon-a11y": "^8.0.9",
    "@storybook/addon-coverage": "^1.0.1",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-viewport": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/nextjs": "^8.0.9",
    "@storybook/react": "^8.0.9",
    "@storybook/react-vite": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^8.0.9",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.4",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-n": "^17.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "postcss": "^8.4.38",
    "postcss-styled-syntax": "^0.6.4",
    "prettier": "^3.2.5",
    "storybook": "^8.0.9",
    "styled-components": "^6.1.8",
    "stylelint": "^16.4.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5",
    "vite": "^5.2.10",
    "yad": "^0.1.3"
  },
  "packageManager": "yarn@4.0.0",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
