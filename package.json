{
  "name": "the-fernande",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:analyze": "ANALYZE=true next build",
    "build": "next build",
    "clean": "rm -rf node_modules .yarn/cache .yarn/install-state.gz .next coverage",
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "lint:check": "echo '--- Running ESLint ---\n' && eslint --color --no-error-on-unmatched-pattern --config ./.eslintrc.json '{public,src}/**/*.{js,jsx,ts,tsx,json}';",
    "lint:fix": "echo '--- Running ESLint ---\n' && eslint --fix --color --config ./.eslintrc.json '{public,src}/**/*.{js,jsx,ts,tsx,json}';",
    "prepare": "husky install",
    "prettier:check": "prettier --check --config ./.prettierrc '{public,src}/**/*.{htm,html,js,jsx,ts,tsx,css,md}'",
    "prettier:fix": "prettier --write --list-different --config ./.prettierrc '{public,src}/**/*.{htm,html,js,jsx,ts,tsx,css,md}'",
    "preview": "next build && next start",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "sonar": "sonar-scanner"
  },
  "dependencies": {
    "next": "14.2.2",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-n": "^17.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-storybook": "^0.8.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.4.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5"
  }
}
